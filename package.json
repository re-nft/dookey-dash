{
  "name": "my-rainbowkit-app",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "e2e": "playwright",
    "format": "run-s format:**",
    "format:fix": "eslint --quiet --fix **/*.{ts,tsx}",
    "format:pretty": "prettier --cache --write **/*.{js,ts,tsx}",
    "prepare": "husky install",
    "prestart": "next build",
    "pre-commit": "lint-staged",
    "pre-push": "playwright test",
    "start": "next start",
    "lint": "run-p lint:**",
    "lint:code": "eslint **/*.{ts,tsx}",
    "lint:type": "tsc --pretty --noEmit -p tsconfig.json"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --quiet --fix",
      "sh -c 'tsc --pretty --noEmit'"
    ],
    "*.{js,json,md,ts,tsx}": [
      "prettier --cache --write"
    ]
  },
  "dependencies": {
    "@rainbow-me/rainbowkit": "^0.8.1",
    "@rainbow-me/rainbowkit-siwe-next-auth": "^0.1.5",
    "ethers": "^5.0.0",
    "next": "^12.1.6",
    "next-auth": "^4.18.10",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "siwe": "^2.1.3",
    "wagmi": "^0.9.0"
  },
  "devDependencies": {
    "@next/env": "^13.1.1",
    "@playwright/test": "^1.29.2",
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.32.0",
    "eslint-config-next": "^13.1.5",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^9.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3",
    "typescript": "^4.7.2"
  }
}
