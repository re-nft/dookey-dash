{
  "name": "my-rainbowkit-app",
  "private": true,
  "version": "0.1.0",
  "engines": {
    "node": ">=18 <19"
  },
  "scripts": {
    "build": "next build",
    "dev": "run-p dev:**",
    "dev:docker": "scripts/run.sh development",
    "dev:next": "next dev",
    "e2e": "playwright",
    "format": "run-s format:**",
    "format:fix": "eslint --quiet --fix **/*.{ts,tsx}",
    "format:pretty": "prettier --cache --ignore-path .gitignore --write .",
    "lint": "run-p lint:**",
    "lint:code": "eslint **/*.{ts,tsx}",
    "lint:type": "tsc --pretty --noEmit -p tsconfig.json",
    "pre-commit": "lint-staged",
    "pre-push": "playwright test",
    "prepare": "husky install",
    "prestart": "next build",
    "start": "next start",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "playwright test -g test.ts"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "npx eslint . --fix --quiet",
      "sh -c 'tsc --pretty --noEmit'"
    ],
    "*.{js,json,md,ts,tsx,yml,yaml}": [
      "prettier --cache --write"
    ]
  },
  "dependencies": {
    "@rainbow-me/rainbowkit": "^0.8.1",
    "@rainbow-me/rainbowkit-siwe-next-auth": "^0.1.5",
    "@tanstack/react-query": "^4.23.0",
    "@zoralabs/nft-hooks": "^1.2.1",
    "axios": "^1.2.5",
    "delegatecash": "^0.3.1",
    "ethers": "^5.7.2",
    "mongoose": "^6.8.4",
    "next": "^13.1.5",
    "next-auth": "^4.18.10",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-twitter-embed": "^4.0.4",
    "siwe": "^2.1.3",
    "use-delegatecash": "^0.1.4",
    "wagmi": "^0.9.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@next/env": "^13.1.1",
    "@playwright/test": "^1.29.2",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/builder-webpack5": "^6.5.15",
    "@storybook/manager-webpack5": "^6.5.15",
    "@storybook/react": "^6.5.15",
    "@storybook/testing-library": "^0.0.13",
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^8.3.0",
    "eslint": "^8.15.0",
    "eslint-config-next": "^12.1.6",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^9.0.0",
    "eslint-plugin-storybook": "^0.6.10",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "node-mocks-http": "^1.12.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "sass": "^1.57.1",
    "storybook-addon-next": "^1.6.9",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.7.2",
    "webpack": "^5.75.0"
  }
}
